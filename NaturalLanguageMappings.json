{
  "version": "1.0",
  "description": "Natural language to PowerShell command mappings. Edit this file to add new translations without modifying the profile.",
  "mappings": {
    "commands": {
      "list files": "Get-ChildItem",
      "show files": "Get-ChildItem",
      "dir": "Get-ChildItem",
      "ls": "Get-ChildItem",
      "show processes": "Get-Process",
      "list processes": "Get-Process",
      "running processes": "Get-Process",
      "show services": "Get-Service",
      "list services": "Get-Service",
      "current directory": "Get-Location",
      "where am i": "Get-Location",
      "pwd": "Get-Location",
      "system info": "Get-ComputerInfo",
      "computer info": "Get-ComputerInfo",
      "disk space": "Get-PSDrive -PSProvider FileSystem",
      "disk usage": "Get-PSDrive -PSProvider FileSystem",
      "network adapters": "Get-NetAdapter",
      "ip address": "Get-NetIPAddress",
      "my ip": "Get-NetIPAddress -AddressFamily IPv4",
      "date": "Get-Date",
      "time": "Get-Date -Format 'HH:mm:ss'",
      "today": "Get-Date -Format 'yyyy-MM-dd'",
      "uptime": "Get-Uptime",
