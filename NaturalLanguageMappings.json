      "modules": "Get-Module",
      "loaded modules": "Get-Module",
      "help": "Get-Help",
      "clear screen": "Clear-Host",
      "cls": "Clear-Host"
    },
    "patterns": {
      "open (\\w+)": "Start-Process $1",
      "run (\\w+)": "Start-Process $1",
      "launch (\\w+)": "Start-Process $1",
      "start (\\w+)": "Start-Process $1",
      "go to (.+)": "Set-Location '$1'",
      "cd (.+)": "Set-Location '$1'",
      "change directory to (.+)": "Set-Location '$1'",
      "navigate to (.+)": "Set-Location '$1'",
      "read (.+)": "Get-Content '$1'",
      "show (.+) contents": "Get-Content '$1'",
      "cat (.+)": "Get-Content '$1'",
      "find (.+)": "Get-ChildItem -Recurse -Filter '*$1*'",
      "search for (.+)": "Get-ChildItem -Recurse -Filter '*$1*'",
      "ping (.+)": "Test-Connection '$1' -Count 4",
      "test connection to (.+)": "Test-Connection '$1' -Count 4",
      "create folder (.+)": "New-Item -ItemType Directory -Path '$1'",
      "make directory (.+)": "New-Item -ItemType Directory -Path '$1'",
      "mkdir (.+)": "New-Item -ItemType Directory -Path '$1'",
      "create file (.+)": "New-Item -ItemType File -Path '$1'",
      "touch (.+)": "New-Item -ItemType File -Path '$1'",
      "delete (.+)": "Remove-Item '$1'",
      "remove (.+)": "Remove-Item '$1'",
      "copy (.+) to (.+)": "Copy-Item '$1' '$2'",
      "move (.+) to (.+)": "Move-Item '$1' '$2'",
      "rename (.+) to (.+)": "Rename-Item '$1' '$2'",
      "zip (.+)": "Compress-Archive -Path '$1' -DestinationPath '$1.zip'",
      "unzip (.+)": "Expand-Archive -Path '$1'",
      "kill process (.+)": "Stop-Process -Name '$1'",
      "stop (.+)": "Stop-Process -Name '$1'"
    },
